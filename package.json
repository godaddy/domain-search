{
  "name": "domain-search",
  "version": "4.1.0",
  "description": "React-based domain search widget used for designing and building custom GoDaddy reseller storefronts",
  "main": "dist/build/static/js/bundle.min.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:react && npm run build:bundle",
    "build:react": "react-scripts build",
    "build:bundle": "webpack --config webpack.config.js",
    "lint": "node node_modules/eslint/bin/eslint.js --config node_modules/eslint-config-react-app/index.js src/*",
    "test": "react-scripts test --env=jsdom --setupTestFrameworkScriptFile=./src/tests/setupTest.js",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy/domain-search"
  },
  "homepage": "https://godaddy.github.io/domain-search",
  "author": "GoDaddy.com Operating Company, LLC",
  "contributors": [
    "Chris Beresford <cberesford@godaddy.com>",
    "Bryan Focht <bxfocht@godaddy.com>"
  ],
  "keywords": [
    "domains",
    "domain search",
    "reseller",
    "npm",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "fetch-jsonp": "^1.1.3",
    "gh-pages": "^2.0.1",
    "prop-types": "^15.6.1",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-addons-test-utils": "^15.6.2",
    "react-app-polyfill": "^0.2.2",
    "react-dom": "^16.7.0",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "^16.7.0",
    "sinon": "^7.2.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-polyfill": "^1.1.3",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {},
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
