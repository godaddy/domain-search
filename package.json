{
  "name": "domain-search",
  "version": "1.0.2",
  "description": "React-based domain search widget used for designing and building custom GoDaddy reseller storefronts",
  "main": "dist/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "node ./postbuild.js",
    "lint": "node node_modules/eslint/bin/eslint.js --config node_modules/eslint-config-react-app/index.js src/*",
    "test": "react-scripts test --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy/domain-search"
  },
  "homepage": "https://godaddy.github.io/domain-search",
  "author": "GoDaddy.com Operating Company, LLC",
  "contributors": [
    "Chris Beresford <cberesford@godaddy.com>",
    "Bryan Focht <bxfocht@godaddy.com>"
  ],
  "keywords": [
    "domains",
    "domain search",
    "reseller",
    "npm",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "gh-pages": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.14",
    "react-test-renderer": "^16.0.0",
    "sinon": "^4.0.0",
    "whatwg-fetch": "^2.0.3"
  }
}
